    "nodemailer": "^6.9.15",
    "react-fast-marquee": "^1.6.5",
    "sanity": "^3.57.4",
    "styled-components": "^6.1.8",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
